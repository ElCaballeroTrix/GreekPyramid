script/source = "extends KinematicBody2D

var velocidad=Vector2.ZERO
var knockback=Vector2.ZERO
onready var stats=$Stats
export var accelaration=300
export var max_speed=200
export var friction=200
onready var animacion=get_node(\"AnimationPlayer\")
onready var Jugador=get_tree().get_root().get_node(\"Main_Gorgona/Jugador\")
onready var JugadorHurtbox=get_tree().get_root().get_node(\"Main_Gorgona/Jugador/Hurtbox\")
onready var Jugador_Direcc=get_tree().get_root().get_node(\"Main_Gorgona/Jugador/Sprite\")
onready var Jugador_HitAbajo=get_tree().get_root().get_node(\"Main_Gorgona/Jugador/Hit_Abajo/CollisionShape2D\")
var health=1
var Jugador_Encima=false
var Encima=false
var dentroZonaStun=false
enum{
	IDLE,
	ARCO,
	STUN,
	COLETAZO,
	MOVIMIENTO
}
var direccion=-1
var direccion2=Vector2(-1,0)
var state=ARCO
var state_previo=ARCO
var state_siguiente=ARCO
var timer=0
var timer2=0
var mirado=0
var fuera_stun=false
var en_movimiento=0
var coletazo_accion=false
var scale2=1 #Esto se hace, ya que al cambiar el scale.x de un Kinematic2D, move_and_slide, lo vuelve a poner a 1
var zona_Inmune_En_Stun=false
var golpeao_En_Stun=false
var enEstadoArco=false
signal fin_stun()
signal dead()

func _ready():
	stats.connect(\"no_health\",self,\"dead\")
	#animacion.play(\"Idle\")

func _physics_process(delta):
	match state:
		ARCO:
			arco()
			mirado=0
			fuera_stun=false
		STUN:
			if mirado==0:
				stun()
			elif mirado==1: colision_stun()
		COLETAZO:
			coletazo()
		MOVIMIENTO:
			movimiento(delta)
			mirado=0
			fuera_stun=false


func arco():
	if !enEstadoArco:
		enEstadoArco=true
		if coletazo_accion==false:
			#yield(get_tree().create_timer(1),\"timeout\")
			$TimerArco.start()
		else:
			coletazo_accion=false
			animacion.play(\"arco\")

func _on_TimerArco_timeout():
	coletazo_accion=false
	animacion.play(\"arco\")

func flechas_atacan():
	if $Spawn_Node!=null:
		$Spawn_Node/Spawn/Timer.start()

func arco_finished():
	state_siguiente=state
	state=STUN
	enEstadoArco=false

func stun():
	animacion.play(\"Stun_postura\")
func mirando_a_gorgona():
	if mirado==0:
		mirado=1
		$Dano_Stun/Stun_Animacion.play(\"Stun_Ataque\")

func colision_stun():
	if !zona_Inmune_En_Stun:
		if Jugador_Direcc.flip_h==false and scale2==1:
			$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",false)
			golpeao_En_Stun=true
		elif Jugador_Direcc.flip_h==true and scale2==1:
			$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",true)
			golpeao_En_Stun=false
		elif Jugador_Direcc.flip_h==true and scale2==(-1):
			$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",false)
			golpeao_En_Stun=true
		elif Jugador_Direcc.flip_h==false and scale2==(-1):
			$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",true)
			golpeao_En_Stun=false

func zona_Inmune_Stun():
	zona_Inmune_En_Stun=true

func visible_stun():
	$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",true)
	emit_signal(\"fin_stun\")
	zona_Inmune_En_Stun=false
	$Luz_Cabeza.energy=0
	animacion.play(\"Idle\")
	if golpeao_En_Stun and dentroZonaStun: 
		JugadorHurtbox.start_invincibility(0.8)
		golpeao_En_Stun=false
		dentroZonaStun=false
	state=state_siguiente

func movimiento(delta):
	if state_previo==ARCO: 
		animacion.stop(false)
	else:
		animacion.stop()
	#$Animacion_Coletazo.stop()
	$Dano_Stun/Stun_Animacion.stop()
	$Dano_Stun.visible=false
	$Luz_Cabeza.visible=false
	$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",true)
	$Animacion_Coletazo.play(\"Correr\")
	en_movimiento=1
	coletazo_accion=true
	velocidad.x=(max_speed*delta*direccion)/delta
	velocidad.y=0
	velocidad=move_and_slide(velocidad,Vector2(0,-1))

func dead():
	$TimerArco.stop()
	$Timer_Movimiento.stop()
	velocidad=Vector2.ZERO
	enEstadoArco=true
	en_movimiento=1
	$Animacion_Coletazo.stop()
	$Luz_Cabeza.energy=0
	emit_signal(\"dead\")

func _on_Cuerpo_area_entered(area):
	stats.health-=area.damage
	$Health_Boss/ProgressBar.set_bar_value($Health_Boss/ProgressBar.value-1)
	$Cuerpo.start_invincibility(0.4)
	#Si le das con la espada en el aire encima de la Gorgona, se movera
	if Jugador_Encima==true && en_movimiento==0 && !Jugador_HitAbajo.disabled:
		empezar_movimiento_por_jugador_estar_arriba()


func _on_Cuerpo_body_entered(_body):
	if en_movimiento==0:
		coletazo_accion=true
		if state==COLETAZO:
			state_previo=ARCO
		else: 
			state_previo=state
		state=COLETAZO

func empuje():
	Jugador.emit_signal(\"luchando_con_boss\",170,20,direccion)
func coletazo():
	if state_previo==ARCO: 
		animacion.stop(false)
	else: 
		animacion.stop()
		$Dano_Stun/Stun_Animacion.stop()
		#yield(get_node(\"Dano_Stun/Stun_Animacion\"),\"animation_finished\")
	$Dano_Stun/Stun_Animacion.stop()
	$Dano_Stun.visible=false
	$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",true)
	$Luz_Cabeza.visible=false
	$Timer_quitar_empuje.start()
	state_siguiente=ARCO
	$Animacion_Coletazo.play(\"Nivel 1Coletazo\")

func state_anterior():
	enEstadoArco=false
	state=state_previo
	$Dano_Stun.visible=true
	$Luz_Cabeza.visible=true

func _on_Timer_quitar_empuje_timeout():
	Jugador.emit_signal(\"fin_de_empuje_boss\")

func _on_Cuerpo_invincibility_started():
	$BlinkAnimationPlayer.play(\"Start\")

func _on_Cuerpo_invincibility_ended():
	$BlinkAnimationPlayer.play(\"Stop\")

func fin_movimiento():
	$Animacion_Coletazo.stop()
	velocidad=Vector2.ZERO
	if scale.x==-1:
		scale.x=1
	elif scale.x==1:
		scale.x=-1
	#scale.x=scale.x*(-1)
	direccion=direccion*(-1)
	direccion2=Vector2(direccion,0)
	en_movimiento=0
	$Cuerpo/CollisionShape2D.scale.x=1
	$Cuerpo_Dano/CollisionShape2D.scale.x=1
	$Cuerpo/CollisionShape2D.position.x=16
	$Cuerpo_Dano/CollisionShape2D.position.x=16
	$Timer_Movimiento.start(20)
	Encima=false
	Jugador_Encima=false
	if state_previo==STUN || state_previo==COLETAZO:
		state_previo=ARCO
	state_siguiente=ARCO
	state_anterior()

func _on_Timer_Movimiento_timeout():
	$Timer_Movimiento.stop()
	if en_movimiento==0:
		if !Encima:
			yield(get_tree().create_timer(2),\"timeout\")
		else: yield(get_tree().create_timer(1),\"timeout\")
		en_movimiento=1
		$Cuerpo/CollisionShape2D.scale.x=0.4
		$Cuerpo_Dano/CollisionShape2D.scale.x=0.4
		$Cuerpo/CollisionShape2D.position.x=5
		$Cuerpo_Dano/CollisionShape2D.position.x=5
		state_previo=state
		if state_previo==STUN:
			#state_siguiente=MOVIMIENTO
			#yield(get_node(\"Dano_Stun/Stun_Animacion\"),\"animation_finished\")
			$Dano_Stun/Stun_Animacion.seek(0,true)
			$Dano_Stun/CollisionShape2D.set_deferred(\"disabled\",true)
			$Dano_Stun/Stun_Animacion.stop()
			zona_Inmune_En_Stun=false
			$Luz_Cabeza.energy=0
			emit_signal(\"fin_stun\")
		scale2*=-1
		state=MOVIMIENTO
	else: $Timer_Movimiento.start(5)

func empezar_movimiento_por_jugador_estar_arriba():
	if Encima==false:
		$Timer_Movimiento.stop()
		$Timer_Movimiento.start(0.1)
		Encima=true

func _on_Arriba_body_entered(body):
	if body.name==\"Jugador\":
		Jugador_Encima=true


func _on_Arriba_body_exited(body):
	if body.name==\"Jugador\":
		Jugador_Encima=false
"

